{
  "name": "pkgraph",
  "version": "0.0.1",
  "author": "Wang Guan <momocraft@gmail.com>",
  "index": "build/lib/index",
  "bin": {
    "example-bin": "bin/example-bin"
  },
  "files": [
    "bin",
    "build",
    "!build/__test__/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jokester/pkgraph.git"
  },
  "scripts": {
    "build": "rm -rvf build && tsc",
    "local:install": "yarn build && yarn global add $PWD",
    "test": "DEBUG='lockdiff:*' jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --watch --noEmit",
    "lint": "gts lint",
    "lint:fix": "gts fix"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@snyk/dep-graph": "^2.7.0",
    "date-fns": "^2.28.0",
    "debug": "^4.3.4",
    "fp-ts": "<3",
    "lodash-es": "^4.17.21",
    "rxjs": "^7.5.5",
    "snyk-nodejs-lockfile-parser": "^1.52.1",
    "snyk-resolve-deps": "^4.7.3",
    "snyk-tree": "^1.0.0",
    "tslib": "^2",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.66",
    "@swc/jest": "^0.2.26",
    "@types/debug": "^4.1.7",
    "@types/jest": "^29",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gts": "^3.1.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.1"
  }
}
