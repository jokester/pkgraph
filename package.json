{
  "name": "node-lib",
  "version": "0.1.0",
  "author": "Wang Guan <momocraft@gmail.com>",
  "private": true,
  "index": "build/lib/index",
  "bin": {
    "example-bin": "bin/example-bin"
  },
  "files": [
    "bin",
    "build",
    "!build/__test__/"
  ],
  "scripts": {
    "start": "node build/bin/example-bin",
    "build": "rm -rvf build && tsc",
    "local:install": "yarn build && yarn global add $PWD",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --watch --noEmit",
    "lint": "gts lint",
    "lint:fix": "gts fix"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@snyk/dep-graph": "^2.6.1",
    "date-fns": "^2.28.0",
    "debug": "^4.3.4",
    "fp-ts": "<3",
    "lodash-es": "^4.17.21",
    "rxjs": "^7.5.5",
    "snyk-nodejs-lockfile-parser": "^1.51.1",
    "snyk-resolve-deps": "^4.7.3",
    "snyk-tree": "^1.0.0",
    "tslib": "^2",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/node": "^18",
    "eslint-plugin-import": "^2.26.0",
    "gts": "^3.1.0",
    "jest": "^29",
    "typescript": "^5.1"
  }
}
